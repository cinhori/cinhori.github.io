group :jekyll_plugins do
    gem "jekyll-feed"
    gem "jekyll-remote-theme"
    gem "jekyll-seo-tag"
    gem "jekyll-sitemap"
    gem 'jemoji'
    gem "jekyll-github-metadata"
    gem "jekyll-paginate"
  end

gem 'tzinfo-data', platforms: [:mingw, :mswin, :x64_mingw]

gem 'wdm', '>= 0.1.0' if Gem.win_platform?
